@font-face {
    font-family: 'fluro';
    src: url('{{ 'fluro-semibold-webfont.woff2' | asset_url }}') format('woff2'),
         url('{{ 'fluro-semibold-webfont.woff' | asset_url }}') format('woff');
    font-weight: 500;
    font-style: normal;
    font-display: swap;
}
@font-face {
    font-family: 'inter';
    src: url('{{ 'inter-webfont.woff2' | asset_url }}') format('woff2'),
         url('{{ 'inter-webfont.woff' | asset_url }}') format('woff');
    font-weight: 500;
    font-style: normal;
    font-display: swap;
}
