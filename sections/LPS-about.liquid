{% schema %}
{
  "name": "LPS About",
  "tag": "section",
  "class": "lps-about-section",
  "limit": 1,
  "settings": [
    {
      "type": "header",
      "content": "About"
    },
    {
      "type": "paragraph",
      "content": "Code editable in sections/LPS-about.liquid"
    },
    {
      "type": "header",
      "content": "t:sections.all.padding.section_padding_heading"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_top",
      "default": 0
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 0
    }
  ],
  "presets": [
    {
      "name": "LPS About"
    }
  ]
}
{% endschema %}